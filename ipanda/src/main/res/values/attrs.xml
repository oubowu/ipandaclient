<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PagerIndicator">
        <attr name="hollowCircleStrokeWidth" format="dimension"/>
        <attr name="hollowCircleRadius" format="dimension"/>
        <attr name="hollowCircleColor" format="color|reference"/>
        <attr name="solidCircleColor" format="color|reference"/>
    </declare-styleable>

    <declare-styleable name="FollowRecyclerViewBehavior">
        <attr name="dependenceId" format="reference"/>
        <attr name="followId" format="reference"/>
        <attr name="dividerId" format="reference"/>
    </declare-styleable>

    <declare-styleable name="BarBehavior">
        <attr name="direction" format="enum">
            <enum name="same" value="1"/>
            <enum name="reverse" value="2"/>
        </attr>
        <attr name="dependenceBarId" format="reference"/>
    </declare-styleable>

    <declare-styleable name="FlexImageLayout">
        <attr name="spacing" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SimpleVideoImageView">
        <attr name="wRatio" format="fraction"/>
        <attr name="hRatio" format="fraction"/>
    </declare-styleable>


    <declare-styleable name="VerticalLayout">
        <attr name="vSpacing" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="DescImageView">
        <attr name="textRatio" format="fraction"/>
    </declare-styleable>

</resources>